
@{
    ViewData["Title"] = "Inbox";
    Layout = "~/Views/Shared/_MemberLayout.cshtml";
}

@model List<Entity_Layer.Message>

<div class="container">
    <h1>Gelen Kutusu</h1>

    Hoşgeldiniz: @ViewBag.v
    <br />
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Gönderen</th>
            <th>Konu</th>
            <th>Mesajı aç</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MessageID</td>
                <td>@item.SenderName</td>
                <td>@item.Subject</td>
                <td><a href="#" class="btn btn-primary">Mesajı aç</a></td>
            </tr>
        }
    </table>
</div>
